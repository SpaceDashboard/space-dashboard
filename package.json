{
  "name": "space-dashboard",
  "version": "2.3.0",
  "description": "Collection of happenings in space",
  "author": "Caleb Dudley",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-preview": "yarn build && yarn preview",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "checks-before-push": "yarn lint && yarn format:check && yarn type-check",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish --no-git-tag",
    "update-version": "yarn changeset:version",
    "lint": "eslint --max-warnings 100",
    "type-check": "tsc --noEmit",
    "bundle-visualizer": "npx vite-bundle-visualizer -c vite.config.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrocaleb/content-slider": "^1.0.0",
    "@date-fns/utc": "^2.1.1",
    "@emotion/css": "^11.13.5",
    "@sentry/react": "^8.55.0",
    "@sentry/vite-plugin": "^2.23.1",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.10",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "posthog-js": "^1.295.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.66.1",
    "react-toastify": "^10.0.6",
    "react-tooltip": "^5.30.0",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^22.19.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/recharts": "^1.8.29",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "process": "^0.11.10",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "packageManager": "yarn@4.12.0"
}
