{
  "name": "spacedashboard",
  "version": "1.0.0",
  "description": "Collection of happenings in space",
  "author": "Caleb Dudley",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "tsc && vite build",
    "lint": "eslint --max-warnings 100",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "bundle-visualizer": "npx vite-bundle-visualizer -c vite.config.ts",
    "prepare": "husky",
    "checks-before-push": "yarn lint && yarn format:check && yarn type-check"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.0",
    "@emotion/css": "^11.11.2",
    "@sentry/react": "^8.25.0",
    "@sentry/vite-plugin": "^2.22.0",
    "@tabler/icons-react": "^3.11.0",
    "date-fns": "^4.0.0-beta.1",
    "posthog-js": "^1.155.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.1",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.27.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.7.0",
    "@types/node": "^20.14.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.8",
    "globals": "^15.8.0",
    "husky": "^9.1.1",
    "prettier": "3.3.3",
    "process": "^0.11.10",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
