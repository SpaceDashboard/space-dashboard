{
  "name": "spacedashboard",
  "version": "2.0.0",
  "description": "Collection of happenings in space",
  "author": "Caleb Dudley",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "tsc && vite build",
    "lint": "eslint --max-warnings 100",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check ./src",
    "format:fix": "prettier --write ./src",
    "bundle-visualizer": "npx vite-bundle-visualizer -c vite.config.ts",
    "prepare": "husky",
    "checks-before-push": "yarn lint && yarn format:check && yarn type-check"
  },
  "dependencies": {
    "@astrocaleb/content-slider": "^1.0.0",
    "@date-fns/utc": "^2.1.0",
    "@emotion/css": "^11.13.4",
    "@sentry/react": "^8.34.0",
    "@sentry/vite-plugin": "^2.22.5",
    "@tabler/icons-react": "3.17.0",
    "@tanstack/react-query": "^5.59.13",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "posthog-js": "^1.167.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.0",
    "react-toastify": "^10.0.6",
    "react-tooltip": "^5.27.1",
    "recharts": "^2.13.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@tanstack/react-query-devtools": "^5.59.13",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/recharts": "^1.8.29",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "prettier": "3.3.3",
    "process": "^0.11.10",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
